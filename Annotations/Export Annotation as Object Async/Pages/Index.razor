@page "/"

@using Syncfusion.Blazor.PdfViewerServer
@using Syncfusion.Blazor.PdfViewer
@using Syncfusion.Blazor.Buttons
@using System.IO;


<SfButton OnClick="ExportAnnotAsObject">ExportAnnotationsAsObjectAsync</SfButton>

<SfPdfViewerServer @ref="pdfviewer" DocumentPath="@DocumentPath" Height="100%" Width="100%">
</SfPdfViewerServer>

@code {
    SfPdfViewerServer pdfviewer;
    byte[] data;
    public string DocumentPath = "wwwroot/data/F# Succinctly.pdf";
    dynamic export;

    private async void ExportAnnotAsObject()
    {
        export = await pdfviewer.ExportAnnotationsAsObjectAsync() as dynamic;
        export.pdfAnnotation["0"].stampAnnotations = null;
    }
}
